{
  "name": "artprgrmr/events-calendar-gforms-registration",
  "description": "Use Gravity Forms to handle registration for The Events Calendar events. Updated by artprgrmr",
  "keywords": [
    "wordpress"
  ],
  "type": "wordpress-plugin",
  "version": "0.2.1",
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "Tim Jensen",
      "homepage": "https://www.timjensen.us/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "composer/installers": "^2.1"
  },
  "extra": {
    "installer-paths": {}
  },
  "autoload": {
    "psr-4": {
      "ForwardJump\\ECGF_Registration\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}

